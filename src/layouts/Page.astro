---
import "/sass/main.scss";
import Footer from "../components/Footer.astro";
import Header from "../components/head/Header.astro";
import Layout from "./Base.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<Layout title={title}>
  <Header />
  <main>
    <slot />
  </main>
  <Footer />
</Layout>
